<?xml version="1.0" encoding="UTF-8"?>
<widget name="BarChartWidget" id="CreditManager/Widget/BarChart"
  spec="1.0" width="640" height="640" jsClass="CreditManager.Widget.BarChart"
  x:min-width="640"
  x:min-height="640"
  xmlns:x="http://deepweb.ut.ee/automicrosite/OpenAjaxMetadataExtension"
  xmlns="http://openajax.org/metadata"
  sandbox="true">

  <require type="javascript" src="jquery.js"></require>
  <require type="javascript" src="BarChart.js"></require>
  <require type="javascript" src="jquery.flot.js"></require>
  <require type="javascript" src="jquery.flot.stack.js"></require>

  <title>BarChart</title>

  <content>
    <![CDATA[
    <fieldset> 
      <legend id="__WID__legend">Bar Chart Widget</legend>
      <div id="__WID__widget">
        <div>
          <div id="__WID__bar" style="float:left; width:375px; height: 475px;"></div>
          <p id="__WID__barchoices" style="float:right; width:135px;"></p>
          <p id="__WID__barfilter" style="float:right; width:135px;"></p>
        </div>
      </div>
    </fieldset>
    ]]>
  </content>

  <categories>
    <category x:iri="http://deepweb.ut.ee/BarChart" />
  </categories>
</widget>